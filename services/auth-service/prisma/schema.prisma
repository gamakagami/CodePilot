generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                String   @id @default(cuid())
  githubId          String   @unique
  name              String
  email             String?  @unique
  avatarUrl         String?
  githubAccessToken String?
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}
